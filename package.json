{
	"name": "vscode-smoketest-check",
	"displayName": "vscode-smoketest-check",
	"version": "0.0.1",
	"private": false,
	"description": "A test extension used for smoke testing VS Code Extensions UI ",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"main": "./dist/extension.js",
	"browser": "./dist/extension.js",
	"scripts": {
		"compile": "webpack",
		"package": "webpack --mode production --devtool hidden-source-map",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"vscode:prepublish": "yarn run package",
		"watch": "webpack --watch"
	},
	"contributes": {
		"commands": [
			{
				"command": "vscode-smoketest-check.smoketestCheck",
				"title": "Smoke Test Check"
			}
		]
	},
	"activationEvents": [
		"onCommand:vscode-smoketest-check.smoketestCheck"
	],
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"@types/vscode": "^1.57.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"ts-loader": "^9.2.2",
		"typescript": "^4.3.2",
		"typescript-esbuild": "latest",
		"vscode-test": "^1.5.2",
		"webpack": "^5.38.1",
		"webpack-cli": "^4.7.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
